{"ast":null,"code":"import './ProductPage.css';\nimport navBar from \"@/components/NavBar.vue\";\nimport FooterSection from \"@/components/FooterSection.vue\";\nexport default {\n  data() {\n    return {\n      components: {\n        navBar,\n        FooterSection\n      },\n      product: {\n        imgaeurl: ' ',\n        name: 'productComponant',\n        description: 'This is a description of a product',\n        price: 99.99,\n        avilability: 'in stock',\n        // this will consume a service\n        ratings: '* * * */10',\n        activeTab: 'description'\n      },\n      favourateunfilled: require('@/assets/favourate2png.png'),\n      favourateFilledIn: require('@/assets/favourateFilledIn.png'),\n      iFilled: false,\n      computed: {\n        currentfavuorteIcon() {\n          return this.isFilled ? this.heartFilled : this.heartUnfilled;\n        },\n        methods: {\n          onHover() {\n            if (!this.isFilled) {\n              this.isFilled = true;\n            }\n          },\n          onLeave() {\n            if (!this.isFilled) {\n              this.isFilled = false;\n            }\n          },\n          onClick() {\n            this.isFilled = !this.isFilled;\n          }\n        }\n      },\n      relatedItems: [{\n        id: 1,\n        name: 'Related item 1'\n      }, {\n        id: 2,\n        name: 'Related item 2'\n      }, {\n        id: 3,\n        name: 'Related item 3'\n      }]\n    };\n  }\n};","map":{"version":3,"names":["navBar","FooterSection","data","components","product","imgaeurl","name","description","price","avilability","ratings","activeTab","favourateunfilled","require","favourateFilledIn","iFilled","computed","currentfavuorteIcon","isFilled","heartFilled","heartUnfilled","methods","onHover","onLeave","onClick","relatedItems","id"],"sources":["C:\\Users\\PC\\Documents\\javaApplications\\My_comic_city_project_0.2\\src\\frontend\\src\\components\\productComponat.vue"],"sourcesContent":["<template>\r\n  <div class=\"view_item_page-container\">\r\n    <header class=\"page-header\">\r\n    </header>\r\n    <mian class=\"product-page\">\r\n      <div class=\"product-top\">\r\n        <div class=\"product-image\">\r\n          <img src=\"@/assets/item1.png\" alt=\"product Image\">\r\n        </div>\r\n        <div class=\"product-description\">\r\n          <h2>{{ product.name }}</h2>\r\n          <p><strong>Price:</strong>R{{ product.price }}</p>\r\n          <p>{{ product.description }}</p>\r\n          <div class=\"add-to-wishList\">\r\n            <img src=\"@/assets/favourate2png.png\" alt=\"Image of heart\"\r\n                 @mouseover=\"onHover\"\r\n                 @mouseleave=\"onLeave\"\r\n                 @click=\"onClick\">\r\n\r\n            <!--<img src = \"@/assets/favourateFilledIn.png\" alt = \"Image of heart filled red\">-->\r\n\r\n          </div>\r\n          <!-- start of the form -->\r\n          <form @submit.prevent=\"addToCart\">\r\n            <div class=\"form-group\">\r\n              <label for=\"dropDown\">Add {{ product.name }}</label>\r\n              <select id=\"dropDown\" v-model=\"selectedSize\">\r\n                <option v-for=\"dropDown in dropDowns\" :key=\"dropDown\" :value=\"dropDown\">{{ dropDown }}</option>\r\n              </select>\r\n            </div>\r\n\r\n            <div class=\"form-group\">\r\n              <label for=\"quantity\">Quantity</label>\r\n              <input type=\"number\" id=\"quantity\" v-model=\"quantity\" min=\"1\">\r\n            </div>\r\n\r\n            <div class=\"form -action\">\r\n              <button type=\"submit\">Add to cart</button>\r\n            </div>\r\n          </form>\r\n          <!-- end of form-->\r\n\r\n          <div class=\"in-stock\">\r\n            <p>Aviliability:{{ product.avilability }}</p>\r\n          </div>\r\n\r\n          <!-- Tabs section -->\r\n          <div class=\"product-tabs\">\r\n            <ul>\r\n              <li :class=\"{active: activeTab === 'description'}\"\r\n                  @click=\"activeTab='description'\">Description\r\n              </li>\r\n\r\n              <li :class=\"{active: activeTab === 'details'}\"\r\n                  @click=\"activeTab='details'\">Details\r\n              </li>\r\n\r\n              <li :class=\"{active: activeTab === 'ratings'}\"\r\n                  @click=\"activeTab='rating'\">Ratings\r\n              </li>\r\n            </ul>\r\n            <div v-if=\"activeTab === 'description'\">\r\n              <p>{{ product.description }}</p>\r\n              <p>This should show more about the book</p>\r\n            </div>\r\n\r\n            <div v-if=\"activeTab === 'details'\">\r\n              <table>\r\n                <tr v-for=\"(value, key) in productDetails\" :key=\"key\">\r\n                  <td>{{ key }}</td>\r\n                  <td>{{ value }}</td>\r\n                </tr>\r\n              </table>\r\n            </div>\r\n\r\n            <div v-if=\"activeTab === 'ratings'\">\r\n              <p>Customer Ratings: {{ product.ratings }} / 5</p>\r\n            </div>\r\n\r\n          </div>\r\n\r\n          <!-- End of Tabs section -->\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"Related-items\">\r\n        <h3>Related Items</h3>\r\n        <p>This will have a gallery of images that srolls to the left and right</p>\r\n        <!-- I am still going to make the gallery here -->\r\n        <ul>\r\n          <li v-for=\"item in relatedItems\" :key=\"item.id\">{{ item.name }}</li>\r\n        </ul>\r\n      </div>\r\n    </mian>\r\n    <footer class=\"page-footer\">\r\n    </footer>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport './ProductPage.css';\r\nimport navBar from \"@/components/NavBar.vue\";\r\nimport FooterSection from \"@/components/FooterSection.vue\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n\r\n      components: {\r\n        navBar,\r\n        FooterSection,\r\n      },\r\n\r\n      product: {\r\n        imgaeurl: ' ',\r\n        name: 'productComponant',\r\n        description: 'This is a description of a product',\r\n        price: 99.99,\r\n        avilability: 'in stock', // this will consume a service\r\n        ratings: '* * * */10',\r\n        activeTab:'description'\r\n      },\r\n\r\n      favourateunfilled: require('@/assets/favourate2png.png'),\r\n      favourateFilledIn: require('@/assets/favourateFilledIn.png'),\r\n      iFilled: false,\r\n\r\n      computed: {\r\n        currentfavuorteIcon() {\r\n          return this.isFilled ? this.heartFilled : this.heartUnfilled;\r\n        },\r\n\r\n        methods: {\r\n          onHover() {\r\n            if (!this.isFilled) {\r\n              this.isFilled = true;\r\n            }\r\n          },\r\n\r\n          onLeave() {\r\n            if (!this.isFilled) {\r\n              this.isFilled = false;\r\n            }\r\n          },\r\n\r\n          onClick() {\r\n            this.isFilled = !this.isFilled;\r\n          }\r\n        }\r\n\r\n      },\r\n\r\n      relatedItems: [\r\n        {id: 1, name: 'Related item 1'},\r\n        {id: 2, name: 'Related item 2'},\r\n        {id: 3, name: 'Related item 3'}\r\n      ]\r\n    };\r\n  }\r\n};\r\n</script>\r\n\r\n"],"mappings":"AAqGA,OAAO,mBAAmB;AAC1B,OAAOA,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,aAAY,MAAO,gCAAgC;AAE1D,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MAELC,UAAU,EAAE;QACVH,MAAM;QACNC;MACF,CAAC;MAEDG,OAAO,EAAE;QACPC,QAAQ,EAAE,GAAG;QACbC,IAAI,EAAE,kBAAkB;QACxBC,WAAW,EAAE,oCAAoC;QACjDC,KAAK,EAAE,KAAK;QACZC,WAAW,EAAE,UAAU;QAAE;QACzBC,OAAO,EAAE,YAAY;QACrBC,SAAS,EAAC;MACZ,CAAC;MAEDC,iBAAiB,EAAEC,OAAO,CAAC,4BAA4B,CAAC;MACxDC,iBAAiB,EAAED,OAAO,CAAC,gCAAgC,CAAC;MAC5DE,OAAO,EAAE,KAAK;MAEdC,QAAQ,EAAE;QACRC,mBAAmBA,CAAA,EAAG;UACpB,OAAO,IAAI,CAACC,QAAO,GAAI,IAAI,CAACC,WAAU,GAAI,IAAI,CAACC,aAAa;QAC9D,CAAC;QAEDC,OAAO,EAAE;UACPC,OAAOA,CAAA,EAAG;YACR,IAAI,CAAC,IAAI,CAACJ,QAAQ,EAAE;cAClB,IAAI,CAACA,QAAO,GAAI,IAAI;YACtB;UACF,CAAC;UAEDK,OAAOA,CAAA,EAAG;YACR,IAAI,CAAC,IAAI,CAACL,QAAQ,EAAE;cAClB,IAAI,CAACA,Q